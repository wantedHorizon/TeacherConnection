<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eliran.teacherconnection">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".TeacherConnection"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Login" />
        <activity android:name=".Main2Register" />
        <activity android:name=".Workspace" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".AccountSetting" />
        <activity android:name=".SampleRec" />
        <activity android:name=".Conniction.MyConnections" />
    </application>

</manifest>